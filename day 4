#create variable 
a = b = c = 10
quotient = a/10
print("The Quotient is")
print (quotient)
mul = b*50
print("The product of b and 50 is")
print(mul)
sum = c+60
print("The sum of c and 60 is")
print (sum)
# create a string and replace character
string = "Dhoni"
print(string.replace("o" ,"g"))
# convert float into integer
d = 8.6
d = int(d)
print("The converted number is")
print (d)
#convert integer into float
e = 86
